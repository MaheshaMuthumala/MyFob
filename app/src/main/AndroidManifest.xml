<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shehani.thefobnew">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="My Fob"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SocialOptions"   android:label="Social" />
        <activity android:name=".EmailOptions"   android:label="E mail" />
        <activity android:name=".ShoppingCartOption"   android:label="Shopping Cart" />
        <activity android:name=".CloudOptions"   android:label="cloud"></activity>
    </application>

</manifest>